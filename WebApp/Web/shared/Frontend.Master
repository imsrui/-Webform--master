                <a href="index.html">
                    <img src="assets/images/logo.jpg" width="214" height="86" /></a>
            </div>
            <div class="nav_r">
                <ul>
                    <asp:Repeater ID="RepWebMenusList" runat="server">
                        <ItemTemplate>
                            <li><a href='<%#Eval("WebMenus_Link") %>'><%#Eval("WebMenus_Title") %></a></li>
                        </ItemTemplate>
                    </asp:Repeater>
                </ul>
                <div class="curBg"></div>
